@model Koggo.Client.Models.Home.ServicesModel

<!DOCTYPE html>
<html>
<head>
    <title>List of Objects</title>
    <style>
        body {
            font-family: Arial, sans-serif;
        }
        h1 {
            text-align: center;
        }
        table {
            width: 100%;
            border-collapse: collapse;
            margin: 20px 0;
        }
        th, td {
            padding: 10px;
            text-align: center;
            vertical-align: middle;
        }
        th {
            background-color: #f2f2f2;
        }
        img {
            max-width: 100px; /* Define the maximum width for the icons */
            max-height: 100px; /* Define the maximum height for the icons */
            display: block; /* Remove any extra spacing below the images */
            margin: 0 auto; /* Center the images horizontally */
        }
    </style>
</head>
<body>
    <h1>List of Objects</h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Icon</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.Services)
            {
                <tr>
                    <td>@item.Name</td>
                    <td><img src="data:image/png;base64,@item.IconImage" alt="@item.Name" /></td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
